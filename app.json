{
  "expo": {
    "name": "iQueue",
    "slug": "queue_system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iQueueImage.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iQueueImage.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meioseii.queue_system"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iQueueImage.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e27d8a95-f1f3-47a7-885c-717b024fb277"
      }
    }
  }
}
